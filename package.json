{
  "name": "vrcx",
  "version": "0.0.0",
  "description": "manage your friendship of VRChat",
  "author": "pypy <git@pypy.moe>",
  "main": "assets/main.js",
  "scripts": {
    "start": "electron . --enable-logging",
    "rebuild": "electron-rebuild -f -w vrcx-native,better-sqlite3",
    "dev:main": "mix --mix-config=webpack.mix.main.js",
    "prod:main": "mix --production --mix-config=webpack.mix.main.js",
    "watch:main": "mix watch --mix-config=webpack.mix.main.js",
    "dev:preload": "mix --mix-config=webpack.mix.preload.js",
    "prod:preload": "mix --production --mix-config=webpack.mix.preload.js",
    "watch:preload": "mix watch --mix-config=webpack.mix.preload.js",
    "dev:renderer": "mix --mix-config=webpack.mix.renderer.js",
    "prod:renderer": "mix --production --mix-config=webpack.mix.renderer.js",
    "watch:renderer": "mix watch --mix-config=webpack.mix.renderer.js",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pypy-vrc/VRCX.git"
  },
  "keywords": [
    "vrchat"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pypy-vrc/VRCX/issues"
  },
  "homepage": "https://github.com/pypy-vrc/VRCX#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "better-sqlite3": "^7.1.2",
    "element-plus": "^1.0.2-beta.32",
    "vrcx-native": "file:vrcx-native",
    "vue": "^3.0.6",
    "vue-i18n": "^9.0.0"
  },
  "devDependencies": {
    "@vue/compiler-sfc": "^3.0.6",
    "electron": "^11.3.0",
    "electron-builder": "^22.9.1",
    "electron-rebuild": "^2.3.5",
    "html-webpack-plugin": "^5.2.0",
    "laravel-mix": "^6.0.13",
    "pug": "^2.0.4",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.1.0",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.32.8",
    "sass-loader": "^8.0.2",
    "vue-loader": "^16.1.2"
  },
  "build": {
    "appId": "moe.pypy.vrcx",
    "productName": "VRCX",
    "copyright": "Copyright Â© 2021 pypy",
    "files": [
      "assets/*",
      "!**/node_modules/vrcx-native/bin",
      "!**/node_modules/vrcx-native/build/Release",
      "!**/node_modules/vrcx-native/deps",
      "!**/node_modules/vrcx-native/src"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "asar": true,
    "win": {
      "target": "zip"
    }
  }
}
